{
  "name": "discord-openapi-client",
  "version": "0.0.6",
  "description": "Autogenerated OpenAPI client for discord",
  "scripts": {
    "format": "biome format --write",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "lint:fix:unsafe": "biome lint --unsafe",
    "release": "release-it --ci -i=patch",
    "build": "tsx generator.ts && unbuild --minify"
  },
  "repository": {
    "url": "https://github.com/taskylizard/discord-openapi-client"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@9.5.0",
  "author": "taskylizard",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@taskylizard/biome-config": "^1.0.1",
    "@types/node": "^20.14.11",
    "openapi-typescript": "^7.4.4",
    "release-it": "^17.6.0",
    "release-it-pnpm": "^4.6.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "openapi-fetch": "^0.13.3"
  }
}
