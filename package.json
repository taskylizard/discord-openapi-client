{
  "name": "discord-openapi-client",
  "version": "0.1.1",
  "description": "Autogenerated OpenAPI client for discord",
  "scripts": {
    "format": "prettier -w --cache --log-level=warn generated/",
    "release": "release-it --ci",
    "build": "orval && unbuild"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@9.5.0",
  "author": "taskylizard",
  "license": "MIT",
  "devDependencies": {
    "orval": "^6.31.0",
    "release-it": "^17.6.0",
    "release-it-pnpm": "^4.6.2",
    "unbuild": "^2.0.0"
  }
}
